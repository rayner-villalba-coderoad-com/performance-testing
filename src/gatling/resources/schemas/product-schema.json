{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Product",
  "description": "Get product from Restfuldev API",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 2
    },
    "data": {
      "type": "object",
      "required": ["price"]
    }
  },
  "required": ["name", "data"]
}